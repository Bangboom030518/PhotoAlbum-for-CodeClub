{
  "entrypoint": "index.html",
  "shell": "src/my-login.html",
  "fragments": [
    "src/my-about.html",
    "src/my-admin.html",
    "src/my-app.html",
    "src/my-contact.html",
    "src/my-gallery.html",
    "src/my-home.html",
    "src/my-upload.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "preset": "es5-bundled",
    "js": {
      "minify": false,
      "compile": false
    }
  }]
}